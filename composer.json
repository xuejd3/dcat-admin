{
    "name": "xuejd3/laravel-admin",
    "description": "dcat admin",
    "type": "library",
    "keywords": ["dcat", "laravel", "admin", "grid", "form", "laravel admin"],
    "homepage": "https://github.com/jqhph/dcat-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "jqh",
            "email": "841324345@qq.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "spatie/eloquent-sortable": "^4.4",
        "doctrine/dbal": "^4.2"
    },
    "require-dev": {
        "laravel/dusk": "^8.3",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5.3",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Dcat\\Admin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dcat\\Admin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcat\\Admin\\AdminServiceProvider"
            ]
        }
    }
}
